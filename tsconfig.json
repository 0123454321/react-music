{
    "compilerOptions": {
        "baseUrl": "./",
        "outDir": "./dist/",
        "sourceMap": true,
        "module": "commonjs",
        "target": "es5",
        "jsx": "react-jsx",
        "esModuleInterop": true,
        "paths": {
            "Components/*": ["src/components/*"],
            "Apis/*": ["src/apis/*"],
            "AppContainer/*": ["src/containers/*"],
            "Utils/*": ["src/utils/*"]
        }
    },
    "include": [
        "./src/**/*",
        "./sass.d.ts"
    ]
}